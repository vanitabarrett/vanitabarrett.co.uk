{
  "name": "vanitabarrett.co.uk",
  "version": "1.0.0",
  "description": "Vanita Barrett website",
  "dependencies": {
    "node-sass": "6.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-sass": "node-sass src/assets/styles/site.scss _site/assets/styles/styles.css",
    "watch-sass": "node-sass --watch src/assets/styles/site.scss _site/assets/styles/styles.css",
    "watch-eleventy": "./node_modules/.bin/eleventy --serve",
    "build": "npm run build-sass && ./node_modules/.bin/eleventy",
    "start": "npm-run-all build-sass --parallel watch-eleventy watch-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanitabarrett/vanitabarrett.co.uk.git"
  },
  "author": "Vanita Barrett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanitabarrett/vanitabarrett.co.uk/issues"
  },
  "homepage": "https://github.com/vanitabarrett/vanitabarrett.co.uk#readme"
}
