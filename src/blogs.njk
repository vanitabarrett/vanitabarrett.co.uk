---
title: 'Blogs - Vanita Barrett - Web Developer'
layout: 'layouts/base'
---

{% block content %}
    <div class="blogs max-width">
      {% for blog in collections.blogs %}
        <div class="blogs__item__text">
          <a class="blogs__item" href="/blogs/{{ blog.fileSlug }}"><h2>{{ blog.data.name }}</h2></a>
          <p class="blogs__date">Published {{ blog.data.published_date }}</p>
          <p>{{ blog.data.summary }}</p>
        </div>
      {% endfor %}
    </div>

  </div>
{% endblock %}
